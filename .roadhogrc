{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    }
  },
  "proxy": {
    "/api/test": {
      "target": "http://localhost:53071/",
      "changeOrigin": true,
      "pathRewrite": { "/api/test" : "people/hello" }
    },
    "/people": {
      "target": "http://localhost:53071/",
      "changeOrigin": true,
      "pathRewrite": { "/people" : "people/hello" }
    }
  }
}
